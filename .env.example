# XeloraCloud Discord VPS Bot Configuration
# Copy this file to .env and fill in your values

# Discord Bot Configuration
DISCORD_TOKEN=your_discord_bot_token_here

# Admin Configuration
MAIN_ADMIN_ID=your_discord_user_id_here
VPS_USER_ROLE_ID=your_vps_user_role_id_here

# Docker Configuration
DOCKER_NETWORK_NAME=xeloracloud-network

# Branding Configuration (Optional)
BRAND_NAME=XeloraCloud
LOGO_TOKEN=https://i.imgur.com/xSsIERx.png

# Resource Monitoring (Optional)
CPU_THRESHOLD=90
RAM_THRESHOLD=90

# Database Configuration (Optional)
DB_PATH=./data/vps.db

# Logging Configuration (Optional)
LOG_LEVEL=INFO
LOG_FILE=./logs/bot.log